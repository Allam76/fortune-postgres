  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-0x8890": "^1.0.1",
    "fortune": "^1.7.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.0"
  },
  "files": [
    "dist/",
    "lib/",
    "LICENSE"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "0x8890"
