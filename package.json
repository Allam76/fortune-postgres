{
  "name": "fortune-postgres",
  "description": "Postgres adapter for Fortune.",
  "version": "1.2.3",
  "license": "MIT",
  "author": {
    "email": "0x8890@airmail.cc",
    "url": "http://0x8890.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fortunejs/fortune-postgres"
  },
  "scripts": {
